#nothing

<SimulationCanvas>:
    chainA:chainA
    chainB:chainB
    lineT:lineT
    
    ScatterPlane:
        do_rotation: False
        id: scatterInstance
        Label: 
            id: scatterObject
            text: ""
        SimulationLine:
            id: chainA
        SimulationLine:
            id: chainB
        SimulationLine:
            id: lineT

<SimulationLine>:
    canvas:
        Color:
            rgb: root.color
        Line:
            points: root.fromPos[0], root.fromPos[1],root.toPos[0], root.toPos[1]
    Label:
        text: root.lengthAsString
        pos: -root.width/2 + (root.fromPos[0] + root.toPos[0])/2, -root.height/2 + (root.fromPos[1] + root.toPos[1])/2