#nothing

<SimulationCanvas>:
    chainA:chainA
    chainB:chainB
    lineT:lineT
    frameLeft:frameLeft
    frameTop:frameTop
    frameRight:frameRight
    frameBottom:frameBottom
    scatterInstance:scatterInstance
    xPosSlider:xPosSlider
    yPosSlider:yPosSlider
    
    
    ScatterPlane:
        do_rotation: False
        id: scatterInstance
        Label: 
            id: scatterObject
            text: ""
        SimulationLine:
            id: chainA
        SimulationLine:
            id: chainB
        SimulationLine:
            id: lineT
        SimulationLine:
            id: frameLeft
        SimulationLine:
            id: frameTop
        SimulationLine:
            id: frameRight
        SimulationLine:
            id: frameBottom
    Slider:
        id: xPosSlider
        size_hint_x: .25
        pos: 0,-root.height/2 + 25
        min:0
        max:root.bedWidth
    Slider:
        id: yPosSlider
        size_hint_x: .25
        pos: 400,-root.height/2 + 25
        min:0
        max:root.bedWidth
        

<SimulationLine>:
    canvas:
        Color:
            rgb: root.color
        Line:
            points: root.fromPos[0], root.fromPos[1],root.toPos[0], root.toPos[1]
    Label:
        text: root.lengthAsString
        pos: -root.width/2 + (root.fromPos[0] + root.toPos[0])/2, -root.height/2 + (root.fromPos[1] + root.toPos[1])/2