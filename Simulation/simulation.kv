#nothing

<SimulationCanvas>:
    chainA:chainA
    chainB:chainB
    lineT:lineT
    frameLeft:frameLeft
    frameTop:frameTop
    frameRight:frameRight
    frameBottom:frameBottom
    scatterInstance:scatterInstance
    xPosSlider:xPosSlider
    yPosSlider:yPosSlider
    angleA:angleA
    angleB:angleB
    angleP:angleP
    sled:sled
    
    
    ScatterPlane:
        do_rotation: False
        id: scatterInstance
        Label: 
            id: scatterObject
            text: ""
        SimulationLine:
            id: chainA
        SimulationLine:
            id: chainB
        SimulationLine:
            id: lineT
        SimulationLine:
            id: frameLeft
        SimulationLine:
            id: frameTop
        SimulationLine:
            id: frameRight
        SimulationLine:
            id: frameBottom
        SimulationAngle
            id: angleA
        SimulationAngle
            id: angleB
        SimulationAngle
            id: angleP
        SimulationSled
            id: sled
    Slider:
        id: xPosSlider
        size_hint_x: .25
        pos: 0,-root.height/2 + 25
        min:0
        max:root.bedWidth
    Slider:
        id: yPosSlider
        size_hint_x: .25
        pos: 400,-root.height/2 + 25
        min:0
        max:root.bedHeight
        
<SimulationAngle>:
    Label:
        text: root.angleAsString
        font_size: 35
        pos: root.textPos

<SimulationLine>:
    canvas:
        Color:
            rgb: root.color
        Line:
            points: root.fromPos[0], root.fromPos[1],root.toPos[0], root.toPos[1]
    Label:
        text: root.lengthAsString
        font_size: 35
        pos: -root.width/2 + (root.fromPos[0] + root.toPos[0])/2, -root.height/2 + (root.fromPos[1] + root.toPos[1])/2
        
<SimulationSled>:
    canvas:
        Color:
            rgb: (1,0,0)
        Line:
            points: root.sledPointOne[0] - root.sledWidth/2, root.sledPointOne[1] - root.sledHeight, root.sledPointOne[0],root.sledPointOne[1]
        Color:
            rgb: (0,1,0)
        Line:
            points: root.sledPointOne[0] - root.sledWidth/2, root.sledPointOne[1] - root.sledHeight,root.sledPointTwo[0],root.sledPointTwo[1]
        Color:
            rgb: (0,0,1)
        Line:
            points: root.sledPointOne[0], root.sledPointOne[1], root.sledPointTwo[0], root.sledPointTwo[1]
    Label:
        text: root.slantAsString
        font_size:35
        pos:root.sledPointOne[0],root.sledPointOne[1]-200