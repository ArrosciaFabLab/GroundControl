#nothing

<SimulationCanvas>:
    motorSpacingError:motorSpacingError
    motorVerticalError:motorVerticalError
    sledMountSpacingError:sledMountSpacingError
    sledCGError:sledCGError
    
    cols: 2
    Label:
        text: "this is the canvas"
        size_hint_x: .9
    GridLayout:
        cols: 1
        size_hint_x: .1
        
        Label: 
            text: "Motor Spacing\nError"
        Slider:
            min: -10
            max:10
            value: 0
            id: motorSpacingError
        Label: 
            text: "Motor Vertical\nError"
        Slider:
            min: -10
            max:10
            value: 0
            id: motorVerticalError
        Label: 
            text: "Sled Mount\nSpacing Error"
        Slider:
            min: -10
            max:10
            value: 0
            id: sledMountSpacingError
        Label: 
            text: "Sled CG\nError"
        Slider:
            min: -10
            max:10
            value: 0
            id: sledCGError
        
        Button:
            text: "Reset All"
            on_release: root.resetSliders()
            
