#nothing

<SimulationCanvas>:
    motorSpacingError:motorSpacingError
    motorVerticalError:motorVerticalError
    sledMountSpacingError:sledMountSpacingError
    scatterInstance:scatterInstance
    vertBitDist:vertBitDist
    
    cols: 2
    ScatterPlane:
        do_rotation: False
        id: scatterInstance
        size_hint_x: .9
        
    GridLayout:
        cols: 1
        size_hint_x: .1
        
        Label: 
            text: "Motor Spacing\nError"
        Slider:
            min: -50
            max:50
            value: 0
            id: motorSpacingError
        Label: 
            text: "Motor Vertical\nError"
        Slider:
            min: -50
            max:50
            value: 0
            id: motorVerticalError
        Label: 
            text: "Sled Mount\nSpacing Error"
        Slider:
            min: -50
            max:50
            value: 0
            id: sledMountSpacingError
        Label: 
            text: "Vert Dist To\nBit Error"
        Slider:
            min: -50
            max:50
            value: 0
            id: vertBitDist
        
        Button:
            text: "Reset All"
            on_release: root.resetSliders()
        Button:
            text: "Recompute"
            on_release: root.recompute()

<TestPoint>
